<div>

  <div class="jumbotron jumbotron-fluid py-0 mb-0">
    <div class="container text-center">
      <h1>The Daily Leopard</h1>
    </div>
  </div>
  <nav class="navbar navbar-expand-lg navbar-dark">
    <a class="navbar-brand" href="/"><i class="fas fa-cat fa-3x"></i></a>
    <button class="navbar-toggler" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01"
      aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarColor01">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item">
          <button class="btn top-button nav-link" id="savedArticles"><i class="fas fa-star"></i> Saved Articles</button>
        </li>
        <li class="nav-item">
          <button class="btn btn-secondary nav-link" id="scrapeNew"> <i class="fas fa-star"></i> Scrape For New
            Articles!</button>
        </li>
      </ul>
    </div>
  </nav>
  <div class="container-fluid">
    <div class="row">
      <div id="articles" class="col-8 py-4 card-columns">

        {{#each Article}}

        <div class='card'>
          <img src={{this.img}} class='card-img-top' alt='img'>
          <div class='card-body'>
            <h5 data-id={{this._id}} class='card-text'>{{this.title}}</h5>
            <br>
            <p class='mb-0'><strong>{{this.author}}</strong></p>
            <p class='mb-0'>{{this.date}}</p></br>
            <div>
              <a href={{this.link}} target='_blank' class='btn btn-primary btn-block'>Checkout The Article</a>
              <a class='btn btn-primary btn-block' data-id={{this._id}} id='saveArt'>Save Article</a>
              <a class='btn btn-primary btn-block' data-id={{this._id}} id='deleteArt'>Delete Article</a>
            </div>
          </div>
        </div>

        {{/each}}

      </div>
      <div id="notes" class="col-4 "></div>
    </div>
  </div>
</div>



